{% extends 'base.html.twig' %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
</style>

<div class="example-wrapper">
    <span>Hello, please choose one of specific user to get recent tweets: </span>
    <ul>
        {% for user in users %}
                <li>{{ user }}</li>
        {% endfor %}
    </ul>
</div>
{% endblock %}
